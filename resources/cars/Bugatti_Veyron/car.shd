shader_body
{
    layer0
  {
   map=$trackenvmap
   texgen_s=reflection_map
   texgen_t=reflection_map
   texgen_r=reflection_map
  }
  layer1
  {
    map=body.tga
    blendfunc=dst_alpha src_alpha
    depthwrite=0 
  }
}
