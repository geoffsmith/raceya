shader_shadow
{
   
    layer0
    {
        map=shadow.tga
        shininess=0
        alphafunc=lequal 32
        blendfunc=blend
        specular= 0.5 0.5 1
        diffuse= 0.5 0.5 1
        ambient=0.3 0.3 1
        depthwrite=1
    }
}
