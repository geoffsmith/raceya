shader_grillie {
 	cull=none
 	layer0 {
 		map=gril.tga
 		blendfunc=blend
 		alphafunc=greater 128
 	}
 	layer1 {
 		map=$trackenvmap
 		texgen_s=reflection_map
 		texgen_t=reflection_map
 		texgen_r=reflection_map
 		blendfunc=one_minus_dst_color dst_color
 		alphafunc=greater 128
 		depthwrite=0
 	}
 	layer2 {
 		map=gril.tga
 		emission= 0.3 0.3 0.3 1
 		blendfunc=add
 		alphafunc=greater 128
 		depthwrite=0
 	}
 }
shader_grillie 
 	cull=none
 	layer0 {
 		map=gril 2.tga
 		blendfunc=blend
 		alphafunc=greater 128
 	}
 	layer1 {
 		map=$trackenvmap
 		texgen_s=reflection_map
 		texgen_t=reflection_map
 		texgen_r=reflection_map
 		blendfunc=one_minus_dst_color dst_color
 		alphafunc=greater 128
 		depthwrite=0
 	}
 	layer2 {
 		map=gril 2.tga
 		emission= .1 .1 .1 .1
 		blendfunc=add
 		alphafunc=greater 230
 		depthwrite=0
        }
 }
