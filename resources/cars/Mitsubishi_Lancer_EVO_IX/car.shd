shader_body
{
  layer0
 {
  map=fresnel.tga
  texgen_s=sphere_map
  texgen_t=sphere_map
  emission=1 1 1 1
  mipmap=0
 }
 layer1
 {
  map=$trackenvmap
  texgen_s=reflection_map
  texgen_t=reflection_map
  texgen_r=reflection_map
  emission=0.5 0.5 0.5
  blendfunc=filter
  shininess=24
  specular=0.75 0.75 0.75
  mipmap=0
 }
 layer2
 {
  map=body.tga
  blendfunc=one src_alpha
  shininess=24
  specular=0.15 0.15 0.15 0.15
  mipmap=0
 }
 layer3
 {
  map=shine.bmp
  texgen_s=sphere_map
  texgen_t=reflection_map
  texgen_r=reflection_map
  blendfunc=dst_color one 
  shininess=24
  specular=1 1 1 1
  }
}
