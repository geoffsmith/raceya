shader_clear
{
	sort_offset=1
	layer0
	{
		texenv=replace
		map=clear.tga
		blendfunc=zero src_alpha 
	}
	layer1
	{
		texenv=replace
		map=fresnel2.tga
		texgen_s=sphere_map
		texgen_t=sphere_map
		blendfunc=one dst_alpha
	}
	layer2
	{
		texenv=replace
		map=$trackenvmap
		texgen_s=reflection_map
		texgen_t=reflection_map
		texgen_r=reflection_map
		blendfunc=dst_alpha one
	}
}

